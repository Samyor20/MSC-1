//2. Take multiple files as Command Line Arguments and print their inode number

#include<stdio.h>
#include<sys/stat.h>
#include<stdlib.h>

void main(int argc, int argv[]) 
{
    printf("total number of command line arguments are: %d", argc);

    int i;
    for(int i=0;i<argv;i++) {
        printf("command arguments are: %d", argv);
    }
    for(int i=0; i<argv;i++) {
        printf("file inode number = %d", file_st.st_ino);
    exit(0);
}
}

    
    
